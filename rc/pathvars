export PYENV_ROOT="$HOME/.pyenv"
export PATH="$PYENV_ROOT/bin:$PATH"

# Far too slow!
if command -v pyenv 1>/dev/null 2>&1; then
  eval "$(pyenv init -)"
fi

export PATH="$HOME/bin:$PATH"

# Eclipse
export PATH="$HOME/eclipse/active:$PATH"

# Mossy
export PATH="$PATH:/apollo/env/Mossy/bin"

