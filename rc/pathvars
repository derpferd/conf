# NIX
NIX_PROFILE=/nix/var/nix/profiles/per-user/$USER/profile
NIX_ZSH=$NIX_PROFILE/bin/zsh
source /home/beauliej/.nix-profile/etc/profile.d/nix.sh
export PATH=$HOME/.nix-profile/bin:$PATH
